    <resultMap id="RM.Amount" class="foo.bar.com.x.cache.Amount">
        <result property="id" column="id"/>
        <result property="fee" column="fee"/>
     </resultMap>
    <update id="updateAmount">
        update [physicTableName] set fee=#num# where id=1
    </update>
    <update id="updateAmount1">
        update [physicTableName] set fee=#num# where id=1
    </update>
    <select id="queryAmount" resultMap="RM.Amount">
        select * from [physicTableName] where id=#id#
    </select>
    <select id="queryAmount1" resultMap="RM.Amount">
        select * from amount where id=#id#
    </select>
    <select id="queryAmountList" resultMap="RM.Amount">
        select * from [physicTableName] where 1=1
    </select>

</sqlMap>