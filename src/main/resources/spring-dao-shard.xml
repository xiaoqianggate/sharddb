<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jee="http://www.springframework.org/schema/jee"
       xmlns:tx="http://www.springframework.org/schema/tx" xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-3.0.xsd http://www.springframework.org/schema/jee http://www.springframework.org/schema/jee/spring-jee-3.0.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd"
       default-autowire="byName">
    <bean id="xaDbTemplateAccessor" class="foo.bar.com.x.cache.shard.XaDbTemplateAccessor">
        <property name="dsMap" ref="xaDsMap"></property>
        <property name="transactionTemplate" ref="transactionTemplate"></property>
    </bean>
    <bean id="singleDbTemplateAccessor" class="foo.bar.com.x.cache.shard.SingleDbTemplateAccessor">
        <property name="dsMap" ref="singleDsMap"></property>
    </bean>
    <bean id="ibatisXaDbTemplateAccessor" class="foo.bar.com.x.cache.shard.IbatisXaDbTemplateAccessor">
        <property name="dsMap" ref="xaDsMap"></property>
        <property name="transactionTemplate" ref="transactionTemplate"></property>
    </bean>
    <bean id="ibatisSingleDbTemplateAccessor" class="foo.bar.com.x.cache.shard.IbatisSingleDbTemplateAccessor">
        <property name="dsMap" ref="singleDsMap"></property>
    </bean>
</beans>